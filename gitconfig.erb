[user]
  name = Comron Sattari
  email = comron@gmail.com

[alias]
  co = checkout
  st = status
  ci = commit
  br = branch
 
[color]
  diff = auto
  status = auto
  branch = auto

[core]
  excludefile = <%= ENV['HOME'] %>/.gitignore
  excludesfile = <%= ENV['HOME'] %>/.gitignore
  editor = <%= `which vim` %>

[apply]
  whitespce = nowarn

[format]
  pretty = %C(yellow)%h%Creset %s %C(red)(%cr)%Creset

[github]
  user = comron

