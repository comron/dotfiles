	email = comron@gmail.com

[alias]
  co = checkout
  st = status
  ci = commit
 
[color]
  diff = auto
  status = auto
  branch = auto

[core]
  excludefile = <%= ENV['HOME'] %>/.gitignore
  editor = <%= `which vim` %>

[apply]
  whitespce = nowarn

[format]
  pretty = %C(yellow)%h%Creset %s %C(red)(%cr)%Creset
